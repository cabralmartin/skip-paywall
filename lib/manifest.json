{
  "name": "Skip Paywall",
  "version": "0.0.1",
  "description": "Skip Paywall for lanacion.com",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["https://*.lanacion.com.ar/*"],
    "js": ["content.js"],
    "run_at": "document_idle"
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "cookies",
    "*://*/*"
  ]
}